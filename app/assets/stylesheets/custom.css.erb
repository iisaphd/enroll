/* ==========================================================================
   1200px
   ========================================================================== */
@media only screen and (max-width: 1200px) {
    ul.link-2{
      font-size: 8px;
      padding-left: 0;
    }
    ul.link-2 li.inline{
      font-size: 8px;
    }
    ul.link-1{
      font-family: 'AvenirLTStd-Light';
      font-size: 8px;
    }
}

/* ==========================================================================
   992px
   ========================================================================== */
@media only screen and (max-width: 992px) {
  .navbar-nav li p{
    font-size: 14px;
    padding-left: 2px;
  }
  .nav-justified li a{
    font-size: 10px;
  }
  ul.link-2{
    font-size: 12px;
    padding-left: 76px;
  }
  ul.link-2 li.inline{
    font-size: 12px;
  }
  ul.link-1{
    font-size: 12px;
    text-align: center;
  }
  .footer-2 p{
    text-align: center;
    font-size: 12px;
  }
  .link-3{
    text-align: center;
    font-size: 12px;
    padding-left: 0;
  }
  .head-2{
  padding-top: 20px;
  }
  .panel-body.left{
    padding-left: 30px !important;
  }
}

/* ==========================================================================
   845px
   ========================================================================== */
@media only screen and (max-width: 992px) {
  .btn-blue{
    margin-top: 30px;
    display: block;
    width: 100%;
  }
  .list-group{
    display: none;
  }
  .emp-sidebar {
    display: none;
  }
  .panel-body.left{
    padding-left: 15px;
    margin-right: 15px;
    padding-bottom: 15px;
  }
  .key p i{
    bottom: -4px !important;
  }
}

/* ==========================================================================
   768px
   ========================================================================== */
@media only screen and (max-width: 768px) {
  .navbar-nav li p{
    display: none;
  }
  .navbar-nav a{
    padding-left: 20px;
  }
  .green-btn{
    display: block;
    width: 100%;
    margin-bottom: 15px;
  }
  .btn-blue{
    display: block;
    width: 100%;
    margin-top: 15px;
  }
/*  .dropdown-menu{
  left: 255px;
  }*/
  li.nav{
    display: none;
  }
  .more{
    margin-bottom: 15px;
  }
 .navbar-fixed-bottom{
    position: absolute !important;
    bottom: 0 !important;
  }
  .key p{
    padding-top: 0 !important;
  }
}

/* ==========================================================================
   624px
   ========================================================================== */
@media only screen and (max-width: 624px) {
  ul.link-2{
    display: none;
  }
  
}
/* ==========================================================================
   385px
   ========================================================================== */
@media only screen and (max-width: 385px) {
  .prev a{
    font-size: 12px;
  }
  .panel-left h4{
    font-size: 14px;
  }
  .panel-left p{
    font-size: 12px;
  }
  .panel-left h4 span{
    top: 7px;
    left: -16px;
  }
  .panel-middle h1{
    font-size: 24px;
  }
  .panel-middle h3{
    font-size: 18px;
  }
  .right{
    text-align: right;
  }
}
/* ==========================================================================
   320px
   ========================================================================== */
@media only screen and (max-width: 320px) {
  .green-btn{
    display: block;
    width: 100%;
    margin-bottom: 15px;
  }
  .navbar-nav a{
    display: none;
  }
  .prev a{
    font-size: 9px;
  }
  .panel-left p{
    font-size: 10px;
  }
  .panel-middle h1{
    font-size: 18px;
  }
  .panel-middle h3{
    font-size: 12px;
  }
  p.abs{
    font-size: 10px;
  }
  li.nav{
    display: none;
  }
}


