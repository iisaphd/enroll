}

/* ==========================================================================
   footer
   ========================================================================== */

footer {
  position: absolute;
  bottom: 0;
  width: 100%;
  /* Set the fixed height of the footer here */
  z-index : 1000;
  height: 2px;
}

/* ==========================================================================
   footer / footer-1
   ========================================================================== */

.footer-1{
  background-color: #ffffff;
  padding-top: 30px;
  padding-bottom: 30px;
}
.footer-1 ul.link-1 li{
  display: inline;
}
ul.link-1{
  font-size: 12px;
  font-weight: 700;
  margin-top: 20px;
}
ul.link-1 li{
  border-right: 1px solid #008ecd;
  padding-right: 10px;
  padding-left: 10px;
}
ul.link-1 li a:hover{
  text-decoration: none;
}
ul.link-1 li:first-child{
  padding-left: 0px;
}
ul.link-1 li:last-child{
  padding-right: 0px;
  border-right: 0px;
}


ul.link-2{
  font-size: 14px;
  display: inline;
  padding-left: 0;
}


ul.link-2{
  list-style: none;
}
ul.link-2 li.inline{
  display: inline-block;
  font-size: 12px;
  font-weight: 700;
}
li.inline ul{
  list-style: none;
  position: relative;
}
li.inline ul li{
  padding-top: 5px;
}
li.inline ul li a:hover{
  text-decoration: none;
}
li.inline ul li a span{
  position: absolute;
  top: 0;
  left: 8px;
  font-size: 28px;
}

/* ==========================================================================
   footer / footer-2
   ========================================================================== */

.footer-2{
  background-color: #008ecd;
  padding-top: 20px;
  padding-bottom: 20px;
}
.footer-2 p, .link-3{
  color: #ffffff;
  font-size: 9px;
  font-weight: 700;
}
.footer-2 ul li{
  display: inline;
  color: #fff;
}
ul.link-3 li a{
  color: #fff;
  border-right: 1px solid #fff;
  padding-right: 10px;
  padding-left: 10px;
}
ul.link-3 li a.insure{
  border-right: 0px !important;
}
ul.link-3 li:first-child{
  padding-left: 0px;
}
ul.link-3 li:last-child{
  padding-right: 0px;
  border-right: 0px;
}

ul.social li a span i{
  font-size: 24px;
  color: #ffffff;
}

/* ==========================================================================
   page-2
   ========================================================================== */
.welcome ul.status{
  padding-left: 0;
}
.welcome ul.status li{
  display: inline;
  padding-right: 10px;
}   
