/* Layout Specifics */
html {
  position: relative;
}
body {
  height : 100%;
  background: none repeat scroll 0 0 #eee;
}

.navbar-fixed-top {
    padding: 0 45px;
}
.navbar-default {
    background-color: #eee;
    background-image: none;
    box-shadow: none;
}
.row {
  margin-left: 0;
  margin-right: 0;
}
h1,h2,h3,h4,h5,h6,p,a{
  font-family: 'AvenirLTStd-Light';
}
.full {
  background-color: #f2f2f2;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
}
.landing .container-fluid {
    padding: 0;
}
.container-fluid > .panel-default {
  border: none;
  box-shadow: none;
}
.landing .panel {
    background: none repeat scroll 0 0 #eee;
    margin-bottom: 0;
}

a:hover, a:focus {
  outline: none;
  text-decoration: none;
}

.focus_effect .disable-btn.disabled {
    background: none repeat scroll 0 0 #ccc;
}

.adderess-select-box + ul {
    border: 1px solid #bdbbbb;
    border-radius: 0;
    left: 0 !important;
    padding: 0;
    width: 100%;
}
.adderess-select-box + ul li {
    border-bottom: 1px solid #bdbbbb;
}
.adderess-select-box + ul li a {
    display: inline-block;
    height: 35px;
    margin: 0 5px;
    padding: 10px 0;
    text-align: center;
}
.adderess-select-box + ul li a:nth-child(2) {
    padding-top: 5px;
    width: 55%;
}
.adderess-select-box + ul li a:last-child {
    float: right;
}
.adderess-select-box + ul li:last-child {
    border-bottom: medium none;
}
.adderess-select-box + ul li:hover {
    background: none repeat scroll 0 0 #f5f5f5;
}

/* Flash Messages */
.alert {
  padding-left: 60px;
  padding-right: 60px;
}

.alert-error {
    background-color: #f2dede;
    border-color: #eed3d7;
    color: #b94a48;
    text-align: left;
 }

.alert-alert {
    background-color: #f2dede;
    border-color: #eed3d7;
    color: #b94a48;
    text-align: left;
 }

.alert-success {
    background-color: #dff0d8;
    border-color: #d6e9c6;
    color: #468847;
    text-align: left;
 }

.alert-notice {
    background-color: #dff0d8;
    border-color: #d6e9c6;
    color: #468847;
    text-align: left;
 }

