@font-face {
    font-family: 'AvenirLTStd-Light';
    src: url('<%= asset_path('AvenirLTStd-Light.eot') %>');
    src: url('<%= asset_path('AvenirLTStd-Light.eot') + '?#iefix' %>') format('embedded-opentype'),
         url('<%= asset_path('AvenirLTStd-Light.woff') %>') format('woff'),
	 url('<%= asset_path('AvenirLTStd-Light.ttf') %>') format('truetype');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'AvenirLTStd-black';
    src: url('<%= asset_path('avenirltstd-black.eot') %>');
    src: url('<%= asset_path('avenirltstd-black.eot') + '?#iefix' %>') format('embedded-opentype'),
         url('<%= asset_path('avenirltstd-black.woff') %>') format('woff'),
	 url('<%= asset_path('avenirltstd-black.ttf') %>') format('truetype');
    font-weight: normal;
    font-style: normal;
}

/*@import url(http://fonts.googleapis.com/css?family=Roboto:400,100,300,500,700);*/

h1,
h2,
h3,
h4,
h5,
h6,
p,
a{
  font-family: 'AvenirLTStd-Light';
}
html {
  position: relative;
  /*min-height: 100%;*/
  /*overvlow: hidden;*/
}
body {
  /* Margin bottom by footer height */
  /* margin-bottom: 60px; */
  height : 100%;
  /* padding-bottom: 210px; */
}
.full {
  background-color: #f2f2f2;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
}

a:hover, a:focus {
  outline: none;
  text-decoration: none;
}

/* ==========================================================================
   nav 1
   ========================================================================== */
.navbar-default{
  background-color: #ffffff;
}
.navbar-nav {
  width: 100%;
  text-align: center;
}
.navbar-nav a{
  padding-top: 10px;
}
.navbar-nav li {
  float: none;
  display: inline-block;
}
.navbar-nav li p{
  font-family: 'AvenirLTStd-Light';
  padding-top: 15px;
  padding-bottom: 15px;
  padding-left: 100px;
  font-size: 18px;
  margin-bottom: 0;
  color: #616161;
}
.navbar-nav li p a{
  color: #009acc;
}
.navbar-nav li p a:hover{
  text-decoration: none;
}
.social.aktif{
  background-color: #007bc4;
}
.social.border{
  border-left: 1px solid #bbbbbb;
}
/*.navbar-default .navbar-nav > li > a:hover{
  background-color: #008ecd;
}*/

/* ==========================================================================
   nav 2
   ========================================================================== */
.nav-justified{
  margin-top: 56px;
}
.nav-justified li a{
  border : 1px solid #dddddd;
  border-radius: 0px !important;
  color: #bbbbbb;
  font-weight: 400;
}
.nav-justified li a:hover{
  border : 0px;
  background-color: #008ecd;
  color: #fff;
}
.nav-justified li a.blue{
  border : 0px;
  background-color: #008ecd;
  color: #fff;
}
.nav-justified li a.green{
  border : 0px;
  background-color: #1cbbb4;
  color: #fff;
}



/* ==========================================================================
   content / form
   ========================================================================== */
.row.content{
  margin: 30px 0 0;
}
.welcome h1{
  font-weight: 300;
  font-size: 30px;
  color: #2b9dd2;
}
.welcome h4{
  font-weight: 400;
  font-size: 14px;
  color: #333333;
}
.welcome h4 span i{
  font-size: 8px;
