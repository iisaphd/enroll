  color: #ed4147; /*red*/
}
span.red{
  position: absolute;
  right: 0;
  top: 0;
  font-size: 8px;
  vertical-align: middle;
  color: #ed4147
}
.head-label{
  font-size: 16px;
  font-weight: 400;
}
.no-pd{
  padding: 0 !important;
}
.no-pd-right{
  padding-right: 0;
}
.no-pd-left{
  padding-left: 0;
}
a.btn.add{
  color: #008ecd;
}
.next{
  margin-top: 30px;
  padding-top: 30px;
  border-top: 1px solid #bbbbbb
}
.btn {
  color: #fff;
  font-size: 18px;
  font-weight: 500;
  padding-right: 30px;
  padding-left: 30px;
  padding-top: 10px;
