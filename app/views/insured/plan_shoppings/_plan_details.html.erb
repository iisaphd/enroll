<div class="plan-row panel panel-default" data-plan-category="<%= plan.plan_type ? plan.plan_type.downcase : "" %>" data-plan-metal-level="<%= plan.metal_level.downcase %>" data-plan-metal-network="<%= plan.nationwide ? "nationwide" : "dc area network" %>" data-plan-metal-premium="<%= plan.total_employee_cost %>" data-plan-metal-deductible="<%= plan.deductible %>" data-plan-name="<%= plan.name %>" data-plan-carrier="<%= plan.try(:carrier_profile).try(:legal_name) %>" data-plan-hsa-eligibility="<%= @plan_hsa_status[plan.id.to_s] %>">
  <div class="panel-body">
      <div class="row">
          <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">
            <h3 class="text-blue">
              <p class="title-inline" style="height: 80px; padding-right: 10px"><%= display_carrier_logo(plan.carrier_profile, {width: 80}) %></p>
              <%= link_to plan.name, summary_products_plans_path({:standard_component_id => plan.hios_id, hbx_enrollment_id: @hbx_enrollment.id}), {:remote => true} %>
              <br/>
              <small><%= (Caches::MongoidCache.lookup(CarrierProfile, plan.carrier_profile_id) { plan.carrier_profile} ).dba %></small>
            </h3>
            <table class="table gn-10">
              <thead>
                  <tr>
                      <th>TYPE</th>
                      <th>LEVEL</th>
                      <th>NETWORK</th>
                      <th>DEDUCTIBLE</th>
                  </tr>
              </thead>
              <tbody>
                  <tr>
                      <td><%= plan.plan_type ? plan.plan_type.upcase : ""%></td>

                      <% plan_level = plan.metal_level.humanize %>
                      <td class="<%= plan_level.downcase %>"><%= plan_level %></td>

                      <td><%= plan.nationwide ? "Nationwide" : "DC Area Network" %></td>
                      <td><%= plan.deductible %></td>
                  </tr>
              </tbody>
            </table>
          </div>

          <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
              <div class="price-wrapper no-pd pull-right">
                  <h2><%= number_to_currency(plan.total_employee_cost) %></h2>
                  <small>/Month</small>
              </div>
          </div>

          <div class="compare-wrapper">
              <div class="checkbox">
                  <label>
                    <input id="compare_plan_checkbox" type="checkbox" class="checkbox-custom select-plan-for-comparison" data-hios-id="<%= plan.hios_id %>" data-plan-year="<%= plan.active_year %>"></input>
                    <lable for="compare_plan_checkbox" class="checkbox-custom-label">Compare</span>
                  </label>
              </div>
              <div class="text-right">
                <%= link_to "Details", summary_products_plans_path({:standard_component_id => plan.hios_id, hbx_enrollment_id: @hbx_enrollment.id}), {:class => "btn btn-default", :remote => true} %> &nbsp;
                <%= link_to("Select Plan", thankyou_insured_plan_shopping_path(@hbx_enrollment, {:plan_id => plan.id, :change_plan => @change_plan}), {:class => "btn btn-select", :method => :post}) %> &nbsp;
		<%= link_to 'Waive', '#', data: {toggle: "modal", target: "#waive_confirm"}, class: 'btn btn-default' %>
              </div>
          </div>
      </div>
  </div>
</div>



