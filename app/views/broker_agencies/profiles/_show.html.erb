<div class="container bottom-pd">

 <h3>Broker Agency : <%= @broker_agency_profile.legal_name %></h3>

 <div class="table-border table-responsive">
  <table class="table">
    <thead>
      <th>Legal Name</th>
      <th>dba</th>
      <th>Fein</th>
      <th>Entity Kind</th>
      <th>Market Kind</th>
    </thead>
    <tbody>
      <td><%= @broker_agency_profile.legal_name %></td>
      <td><%= @broker_agency_profile.dba %></td>
      <td><%= @broker_agency_profile.fein %></td>
      <td><%= @broker_agency_profile.entity_kind.titleize %></td>
      <td><%= @broker_agency_profile.market_kind.titleize %></td>
    </tbody>
  </table>
</div>

<%= render "broker_agency_contacts", broker_agency_contacts: @broker_agency_profile.broker_agency_contacts %>
<%= render "office_locations", office_locations: @broker_agency_profile.organization.office_locations %>
</div>
